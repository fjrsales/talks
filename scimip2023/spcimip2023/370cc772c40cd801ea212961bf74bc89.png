		<script>
; (function () {
    'use strict';
    var leadStatus = getLeadStatus();

    if (!shouldRunExperiment()) {
        return;
    }

    if (leadStatus === 'not_sent') {
        setTimeout(run, 5000);
    } else {
        run();
    }

    function shouldRunExperiment() {

        // Exclude these pages
        if (window.location.href.includes('/privacy')) return false
        if (window.location.href.includes('/tos')) return false
        if (window.location.href.includes('/gov')) return false

        var sessionCookie = getCookie('mktz_seen_lead_collector') ? true : false;
        var promoCodeCookie = getCookie('mktz_pc') || '';
        var answeredInPreviusSession = getCookie('mktz_subscripton_success') ? true : false;

        if ((sessionCookie && leadStatus === 'not_sent') || (!sessionCookie && answeredInPreviusSession)) {
            return false;
        }

        if (sessionCookie && leadStatus === 'success' && !promoCodeCookie.length) {
            return false;
        }

        if (sessionCookie && leadStatus !== 'not_sent' || !sessionCookie && leadStatus === 'not_sent') {
            return true;
        }

        return false;
    }

    function run() {
        insertOverlay();
        handleCloseOverlay();
        setUrls(document.querySelector('input.omni-input-successUrl'), 'success');
        handleFormSubmit();
        onInputFocus();
        displayCorrectStep();
        copyToClipboard();
        setCookie('mktz_seen_lead_collector', true, 10);
    }

    function insertOverlay() {
        var newDiv = document.createElement('div');
        newDiv.classList.add('omniOverlay-container');
        document.body.appendChild(newDiv);

        var overlay = getOverlayMarkup();
        newDiv.innerHTML = overlay;
    }

    function handleCloseOverlay() {
        var overlayContainer = document.querySelector('.omniOverlay-container');
        overlayContainer.addEventListener('click', function (e) {

            if (e.target.classList.contains('close-convert') || e.target.classList.contains('omniOverlay-container')) {
                overlayContainer.remove();
            }
        });
    }


    function setUrls(input, state) {
        /*
        the form needs two hidden inputs(besides other inputs)
        with the value beeing the url where the user will land
        if the request has failed or was succesfully sent.
        This function sets each url on coresponding input.
        */

        var url = new URL(window.location.href);

        url.searchParams.set('lead_success', state);
        input.setAttribute('value', url);
    }

    function getLeadStatus() {
        var queryString = window.location.search;
        var urlParams = new URLSearchParams(queryString);

        return urlParams.get('lead_success') || 'not_sent';
    }

    function displayCorrectStep() {
        var overlayContainer = document.querySelector('.omniOverlay-container');
        if (leadStatus === 'success') {
            var promoCode = getCookie('mktz_pc');

            overlayContainer.querySelector('.omniOverlay__success .omniOverlay__code').setAttribute('data-code', promoCode)
            overlayContainer.querySelector('.omniOverlay__success .omniOverlay__code').textContent = promoCode;

            setCookie('mktz_pc', promoCode, '-1');

            overlayContainer.querySelector('.omniOverlay__success').classList.remove('o-force-hide');
            overlayContainer.querySelector('.omniOverlay__step1').classList.add('o-force-hide');
        }

        if (leadStatus === 'error') {
            overlayContainer.querySelector('.omniOverlay__step1 .omniOverlay__form-title').textContent = 'We encountered an error, please try again';
            overlayContainer.querySelector('.omni-input-name').value = '';
        }
    }

    function handleFormSubmit() {
        console.log('watching')
        var form = document.querySelector('form[name="omniSubscribeForm"]');
        form.addEventListener('submit', function (e) {
            e.preventDefault();

            console.log('submitted')

            var emailInputValue = form.querySelector('.omni-input-name').value;

            if (!isValidEmail(emailInputValue)) {
                insertErrorMessage('Please insert a valid email address');
                return;
            }
            initRequest(emailInputValue, form);
        });
    }

    function initRequest(inputValue, form) {
        var payload = { "email": inputValue };
        var overlayContainer = form.closest('.omniOverlay-container');
        var prodWorkerUrl = 'https://worker-store-kardia-emails.omni-e.workers.dev';
        var options = {
            method: "POST",
            headers: { 'Content-Type': 'application/json' },
            body: JSON.stringify(payload)
        };

        fetch(prodWorkerUrl, options)
            .then(function (response) {
                if (response.status === 200) {
                    return response.json();
                }
            }).then(handleResponse);

        console.log('request sent')

        function handleResponse(response) {
            if (response.status === 'success') {
                setCookie('mktz_pc', response.code, 1);
                setCookie('mktz_subscripton_success', true, 2147483647);
                //form.submit();
                //overlayContainer.hide();
                overlayContainer.querySelector('.omniOverlay__step1').style.display = 'none';
                overlayContainer.querySelector('.omniOverlay__success').style.display = 'block';
                return;
            }

            if (response.status === 'error' && response.message === 'already subscribed') {
                overlayContainer.querySelector('.omniOverlay__step1 .omniOverlay__form-title').textContent = 'This email has been registered already';
                insertErrorMessage('Please insert another email address');
                return;
            }

            if (response.status === 'error') {
                overlayContainer.find('.omniOverlay__step1 .omniOverlay__form-title').textContent = 'We have encountered an error, please try again.';
            }
        }
    }

    function insertErrorMessage(message) {
        var input = document.querySelector('.omni-input-name');
        var errorMessageContainer = document.createElement('div');
        var form = input.closest('form');
        input.classList.add('o-error');

        errorMessageContainer.classList.add('o-error-message');
        errorMessageContainer.textContent = message;

        if (!form.querySelector('.o-error-message')) {
            form.insertBefore(errorMessageContainer, input);
        }
    }

    function onInputFocus() {
        var input = document.querySelector('.omni-input-name');
        var overlayContainer = input.closest('.omni-subscription-overlay');

        input.addEventListener('focus', function () {
            input.classList.remove('o-error');
            if (overlayContainer.querySelector('.o-error-message')) {
                overlayContainer.querySelector('.o-error-message').remove();
            }
        });
    }

    function isValidEmail(emailValue) {
        var emailRegex = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;

        return emailRegex.test(emailValue);
    }

    function copyToClipboard() {
        var overlayContainer = document.querySelector('.omniOverlay-container');

        overlayContainer.querySelector('.omniOverlay__button--copy').addEventListener('click', function () {

            var copiedValue = 'Welcome10';

            navigator.clipboard.writeText(copiedValue)
                .then(function () {
                    overlayContainer.querySelector('.omniOverlay__button--copy').textContent = 'Code was copied succesfully';
                });
        });
    }

    function setCookie(key, value, expiry) {
        var expires = new Date();

        expires.setTime(expires.getTime() + (expiry * 60 * 60 * 1000));
        document.cookie = key + '=' + value + ';path=/' + ';expires=' + expires.toUTCString();
    }

    function getCookie(key) {
        var keyValue = document.cookie.match('(^|;) ?' + key + '=([^;]*)(;|$)');
        return keyValue ? keyValue[2] : null;
    }

    function submitFormAjax() {
        var form = document.querySelector('form[name="omniSubscribeForm"]');
        var overlayContainer = form.closest('.omniOverlay-container');

        form.addEventListener('submit', function (e) {
            e.preventDefault();

            var emailInputValue = form.querySelector('.omni-input-name').value;

            if (!isValidEmail(emailInputValue)) {
                insertErrorMessage('Please insert a valid email address');
                return;
            }

            var payload = { "email": emailInputValue };
            var prodWorkerUrl = 'https://worker-store-kardia-emails.omni-e.workers.dev';
            var options = {
                method: "POST",
                headers: { 'Content-Type': 'application/json' },
                body: JSON.stringify(payload)
            };

            fetch(prodWorkerUrl, options)
                .then(function (response) {
                    if (response.status === 200) {
                        return response.json();
                    }
                }).then(handleResponse);

            function handleResponse(response) {
                if (response.status === 'success') {
                    setCookie('mktz_pc', response.code, 1);
                    setCookie('mktz_subscripton_success', true, 2147483647);
                    form.submit();
                    overlayContainer.hide();
                    return;
                }

                if (response.status === 'error' && response.message === 'already subscribed') {
                    overlayContainer.querySelector('.omniOverlay__step1 .omniOverlay__form-title').textContent = 'This email has been registered already';
                    insertErrorMessage('Please insert another email address');
                    return;
                }

                if (response.status === 'error') {
                    overlayContainer.find('.omniOverlay__step1 .omniOverlay__form-title').textContent = 'We have encountered an error, please try again.';
                }
            }
        });
    }

    function getOverlayMarkup() {
        var styles = getOverlayStyle();
        return '<style>' + styles + '</style>' +
            '   <div class="omni-subscription-overlay">' +
            '       <div class="omniOverlay__form-wrapper">' +
            '            <div class="omniOverlay__step1">' +
            '                <div class="omniOverlay__form-header">Keep up with our news, promotions and product launches.</div>' +
            '                <div class="omniOverlay__form-title"><span>Sign up now and get </span><span>10% off</span></div>' +
            '                <div class="omniOverlay__form">' +
            '                    <form action="https://cl.s12.exct.net/DEManager.aspx" name="omniSubscribeForm" method="post">' +
            '                        <input type="hidden" name="_clientID" value="526001161">' +
            '                        <input type="hidden" name="_deExternalKey" value="8AA51E40-D344-46E6-BE9B-D1DEC58AE2BE">' +
            '                        <input type="hidden" name="_action" value="add">' +
            '                        <input type="hidden" name="_returnXML" value="0">' +
            '                        <input type="hidden" name="_successURL" value="" class="omni-input-successUrl">' +
            '                        <input type="hidden" name="_errorURL" value="https://cloud.kardia.alivecor.com/signup-error">' +
            '                        <input type="text" name="Email" placeholder="Please enter your email" class="omni-input-name">' +
            '                        <input type="hidden" name="source" value="Welcome10-list">' +
            '                        <button type="submit">Subscribe</button>' +
            '                    </form>' +
            '                </div>' +
            '            </div>' +
            '            <div class="omniOverlay__success" style="display: none;">' +
            '                <div class="omniOverlay__form-header">Thank you for signing up!</div>' +
            '                <div class="omniOverlay__form-title"><span>Use this code to get</span><span>10% off</span></div>' +
